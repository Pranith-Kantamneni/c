#include<stdio.h>
void main(){
    int i,n;
    scanf("%d",&n);
    int arr[n];
    for( int i=0;i<n;i++){
        printf("enter the elements");
        scanf("%d",&arr[i]);
    }
    int s;
    printf("enter the search element");
    scanf("%d",&s);
    for(i=0;i<n;i++){
        if(arr[i]==s){
            printf("element is found at index:%d",i);
            break;
        }

    }
    if(i==n){
        printf("element not found");
    }

}
